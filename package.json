{
  "name": "store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development craco start",
    "build": "dotenv -e .env.development --max_old_space_size=4096 craco build",
    "build:beta": "dotenv -e .env.beta --max_old_space_size=4096 craco build",
    "build:release": "dotenv -e .env.release craco build",
    "build:production": "dotenv -e .env.production craco build",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "antd-mobile": "^5.11.1",
    "antd-mobile-icons": "^0.2.2",
    "axios": "^0.27.2",
    "query-string": "^7.1.1",
    "react": "^17",
    "react-dom": "^17",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@craco/craco": "^5.7.0",
    "craco-less": "^2.0.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "postcss-plugin-px2rem": "^0.8.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
